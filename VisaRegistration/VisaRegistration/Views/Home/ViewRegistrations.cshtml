@model List<VisaRegistration.Models.registration>

<h2>Registrations</h2>

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Visa Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Name
                </td>
                <td>
                    @($"{@item.VisaNum1}-{@item.VisaNum2}-{@item.VisaNum3}-{@item.VisaNum4}")
                </td>
            </tr>
        }
    </tbody>
</table>

<button type="button" class="btn btn-default" onclick="window.history.back()">Back</button>